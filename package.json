{
    "name": "spotify-player",
    "displayName": "Spotify Player",
    "description": "Allows users to control their Spotify music through VSCodee",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "spotify-player.helloWorld",
                "title": "Hello World"
            }
        ],
        "views": {
            "explorer": [
                {
                    "type": "webview",
                    "id": "spotify-player",
                    "name": "Spotify"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.2.2",
        "@types/express": "^4.17.21",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "@types/vscode": "^1.90.0",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0",
        "eslint": "^8.57.0"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "crypto-js": "^4.2.0",
        "express": "^4.19.2"
    }
}
